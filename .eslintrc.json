{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
  },
  "globals": {
      "ENV": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "semi": ["error", "always"],
      "indent": ["error", 2],
      "linebreak-style":0,
      "quotes": ["error","single"]  
  }
}
